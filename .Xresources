! To only apply colors to your terminal, for example, prefix
! the color assignment statement with its name. Example:
!
! URxvt*background:            S_base03
URxvt*termName: rxvt-256color
*.background:             S_base03
URxvt.background:     [80]S_base03
*foreground:              S_base0
*fading:                  40
*fadeColor:               S_base03
*cursorColor:             S_base1
*pointerColorBackground:  S_base01
*pointerColorForeground:  S_base1

*color0:                  S_base02
*color1:                  S_red
*color2:                  S_green
*color3:                  S_yellow
*color4:                  S_blue
*color5:                  S_magenta
*color6:                  S_cyan
*color7:                  S_base2
*color9:                  S_orange
*color8:                  S_base03
*color10:                 S_base01
*color11:                 S_base00
*color12:                 S_base0
*color13:                 S_violet
*color14:                 S_base1
*color15:                 S_base3


! rxvt-unicode ---------------------------------------------------------------
URxvt.termName: rxvt
URxvt.transparent: true
URxvt.inheritPixmap: False
URxvt.imLocale: de_DE.UTF-8
URxvt.scrollBar: false
URxvt.scrollBar right: false
URxvt.scrollColor: #000000
URxvt.saveLines: 500
URxvt.urlLauncher:/usr/bin/firefox
URxvt.cursorBlink: true
URxvt.geometry: 100x50
URxvt.fading: 25%
URxvt.font: xft:iosevka:pixelsize=14:style=Regular
urxvt.boldFont: xft:iosevka:pixelsize=14:style=Heavy
urxvt.depth: 32
URxvt.perl-ext-common: default,matcher,clipboard,tabbed,urxvt-colors
URxvt.keysm.Shift-Control-C: perl:clipboard:copy
URxvt.keysm.Shift-Control-V: perl:clipboard:paste
URxvt*borderless: 1
URxvt.matcher.button: 1
urxvt*dynamicColors: on
urxvt*keysm.F12: perl:urxvt-colors
urxvt*keysm.Shift-Control-D: perl:urxvt-colors
urxvt*keysm.F5: ~/.dynamic-colors/bin/dynamic-colors i

! ROFI
! -----------------------------------------------------------------------------

! Use extended color scheme
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   #ffffff,#525d76,#f5f5f5,#5294e2,#ffffff
! Color scheme for urgent row
rofi.color-urgent:                   #ffffff,#dc322f,#f5f5f5,#5294e2,#dc322f
! Color scheme for active row
rofi.color-active:                   #ffffff,#c2cad0,#f5f5f5,#5294e2,#c2cad0
! Color scheme window
rofi.color-window:    argb:ffffffff,#f5f5f5
! Separator style (none, dash, solid)
rofi.separator-style:                solid

